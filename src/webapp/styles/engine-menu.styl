@require '_variables'
@require '_mixins'

.p-engine-menu
  margin 9.5px 0 0
  padding 0
  list-style-type none
  clearfix()

.p-engine-menu-item
  float left
  margin-left 20px

.p-engine-menu-link
  color $color-grey
  text-decoration none
  text-transform capitalize
  &.p-engine-menu-link-active
    color $color-blue
